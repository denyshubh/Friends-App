<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape
            android:shape="rectangle" >

            <gradient
                android:angle="45"
                android:startColor="@color/blue_google"
                android:endColor="@color/prusian_blue"

                />
            <stroke
                android:color="@color/colorAccent"
                android:width="3dp"
                />
            <corners
                android:radius="5dp"
                />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle" >

            <gradient
                android:angle="45"
                android:startColor="@color/blue_google"
                android:endColor="@color/prusian_blue"

                />
            <stroke
                android:color="@color/colorAccent"
                android:width="3dp"
                />
            <corners
                android:radius="5dp"
                />
        </shape>
    </item>
</selector>
